package models;

import java.util.Date;

public class POI {
	private final int id;
	private final float latitude;
	private final float longitude;
	private final String city;
	private final String description;
	private final Date updatedDate;

	public POI(int id, float latitude, float longitude, String city, String description, Date updatedDate) {
		super();
		this.id = id;
		this.latitude = latitude;
		this.longitude = longitude;
		this.city = city;
		this.description = description;
		this.updatedDate = updatedDate;
	}

	public int getId() {
		return id;
	}

	public float getLatitude() {
		return latitude;
	}

	public float getLongitude() {
		return longitude;
	}

	public String getCity() {
		return city;
	}

	public String getDescription() {
		return description;
	}

	public Date getUpdatedDate() {
		return updatedDate;
	}

	@Override
	public String toString() {
		return "id=" + id + ", latitude=" + latitude + ", longitude=" + longitude + ", city='" + city + '\''
				+ ", description='" + description + '\'' + ", updatedDate=" + updatedDate + '}';
	}

}
